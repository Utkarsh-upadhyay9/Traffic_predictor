# API Keys and Configuration
# Copy this file to .env and fill in your actual values

# Auth0 Configuration
AUTH0_DOMAIN=your-tenant.us.auth0.com
AUTH0_API_AUDIENCE=https://simcity-ai-api
AUTH0_ISSUER=https://your-tenant.us.auth0.com/
AUTH0_ALGORITHMS=RS256

# Google Gemini API
GEMINI_API_KEY=your_gemini_api_key_here

# Agentuity Configuration
AGENTUITY_API_KEY=your_agentuity_key_here
AGENTUITY_WEBHOOK_URL=https://your-agentuity-endpoint.com/webhook

# ElevenLabs API
ELEVENLABS_API_KEY=your_elevenlabs_key_here
ELEVENLABS_VOICE_ID=professional_narrator

# MATLAB Service
MATLAB_SERVICE_URL=http://localhost:8001

# Application Settings
ENVIRONMENT=development
DEBUG=True
CORS_ORIGINS=http://localhost:3000,http://localhost:5173

# Database (if needed)
DATABASE_URL=sqlite:///./simcity.db
